<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="login Suite">
	<test name="BothCorrect">
		<parameter name="username" value="student"></parameter>
		<parameter name="password" value="Password123"></parameter>

		<classes>
			<class name="loginTestCases.BothCorrect" />
		</classes>
	</test>

	<test name="BothIncorrect">
		<parameter name="username" value="student1"></parameter>
		<parameter name="password" value="Password122"></parameter>
		<classes>
			<class name="loginTestCases.BothIncorrect" />
		</classes>
	</test>

	<test name="CorrectUserName">
		<parameter name="username" value="student"></parameter>
		<parameter name="password" value="Password13"></parameter>
		<classes>
			<class name="loginTestCases.CorrectUserName" />
		</classes>
	</test>

	<test name="CorrectPasswordTestCase">
		<parameter name="username" value="student1"></parameter>
		<parameter name="password" value="Password123"></parameter>
		<classes>
			<class name="loginTestCases.CorrectPasswordTestCase" />
		</classes>
	</test>

</suite>